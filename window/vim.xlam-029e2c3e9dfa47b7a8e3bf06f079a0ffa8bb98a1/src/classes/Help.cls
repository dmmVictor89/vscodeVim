VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "cls_Help"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Explicit

Public Parent As cls_Vim
Private HELP_DICT_JP As Dictionary
Private HELP_DICT_EN As Dictionary

Public Function GetText(ByVal funcName As String) As String
    If Parent.IsJapanese Then
        Call InitJPDict
        If HELP_DICT_JP.Exists(funcName) Then
            GetText = HELP_DICT_JP(funcName)
        Else
            GetText = funcName
        End If
    Else
        Call InitENDict
        If HELP_DICT_EN.Exists(funcName) Then
            GetText = HELP_DICT_EN(funcName)
        Else
            GetText = funcName
        End If
    End If
End Function

